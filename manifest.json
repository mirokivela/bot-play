{
    "manifest_version": 3,
    "name": "Bot !Play",
    "description": "Automaticly creates a bot command for e.g. Discord from a Youtube page. You can assign the prefix by clicking the extension icon.",
    "version": "1.2.0",
    "icons": 
    { 
    "16": "icons/default/icon16.png",
    "19": "icons/default/icon19.png",
    "32": "icons/default/icon32.png",
    "38": "icons/default/icon38.png",
    "48": "icons/default/icon48.png",
    "128": "icons/default/icon128.png" 
    },
    "content_scripts": [{
            "matches": [
                "https://www.youtube.com/*"
            ],
            "js": [
                "content.js"
            ]}],
    "background": {
        "service_worker": "background.js",
        "persistant": false
    },
    "web_accessible_resources": [{
        "resources": ["icons/default/icon32.png", "icons/grey/icon32.png"],
        "matches": ["<all_urls>"],
       "extension_ids": []
      }], 
    "permissions": ["webNavigation", "storage"],
    "action": {
        "default_icon": "icons/default/icon128.png",
        "default_popup": "popup.html"
    }
}